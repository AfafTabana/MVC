@model List<Course>
<h1>My Courses</h1>
<table class="table table-striped table-hover table-bordered table-sm">
    <thead class="table-dark">
    <tr>
        <th> Id </th>
        <th>Name</th>
        <th>degree</th>
        <th>mindegree</th>
        <th>hours</th>
        <th>DeptId</th>
    </tr>
    </thead>
    @foreach(Course items in @Model)
    {
        <tr>
            <td>@items.Id</td>
            <td>@items.Name</td>
            <td>@items.degree</td>
            <td>@items.mindegree</td>
            <td>@items.hours</td>
            <td>@items.DeptId</td>
        </tr>
    }
</table>

<a href="Course/Add" class="btn btn-dark">Add New Course </a>